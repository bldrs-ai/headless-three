{
  "type": "module",
  "main": "src/headless.js",
  "scripts": {
    "build": "yarn setup && yarn build-headless && yarn build-server",
    "clean": "run-script-os",
    "clean:linux:darwin": "rm -rf ./build && rm -rf ./dist",
    "clean:win32": "rmdir /s /q build && rmdir /s /q dist",
    "build-headless": "yarn build-headless-conway && yarn build-headless-webifc",
    "build-server": "yarn build-server-conway && yarn build-server-webifc",
    "build-headless-conway": "node esbuild/headless.esbuild.js",
    "build-headless-webifc": "node esbuild/headless-webifc.esbuild.js",
    "build-server-conway": "node esbuild/server.esbuild.js",
    "build-server-webifc": "node esbuild/server-webifc.esbuild.js",
    "lint": "eslint",
    "setup": "yarn clean && mkdir build && mkdir dist && cp node_modules/gl/build/Release/webgl.node build && cp node_modules/@bldrs-ai/conway/compiled/dependencies/conway-geom/Dist/* build && cp node_modules/@bldrs-ai/conway/compiled/dependencies/conway-geom/Dist/* dist && cp node_modules/web-ifc/*.wasm build",
    "serve": "node --enable-source-maps build/server.node.js",
    "serve-single-thread": "run-script-os",
    "serve-single-thread:linux:darwin": "FORCE_SINGLE_THREAD='true' yarn serve",
    "serve-single-thread:win32": "set FORCE_SINGLE_THREAD='true'&& yarn serve",
    "serve-webifc": "node --enable-source-maps build/server-webifc.node.js",
    "test": "jest",
    "prepare": "husky"
  },
  "dependencies": {
    "@bldrs-ai/conway-web-ifc-adapter": "0.22.921",
    "@sentry/node": "7.64.0",
    "axios": "1.4.0",
    "camera-controls": "2.7.0",
    "canvas": "^3.1.0",
    "express": "4.18.2",
    "fetch-blob": "4.0.0",
    "gl": "9.0.0-rc.4",
    "jimp": "0.16.1",
    "jsdom": "^26.1.0",
    "nanoid": "5.0.3",
    "node-fetch": "3.3.1",
    "pngjs": "7.0.0",
    "run-script-os": "1.1.6",
    "three": "0.173.0",
    "web-ifc": "0.0.67",
    "web-worker": "1.2.0",
    "winston": "3.11.0",
    "xhr2": "0.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/preset-env": "7.22.10",
    "@babel/preset-typescript": "7.23.2",
    "@eslint/compat": "1.2.5",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.19.0",
    "@jest-mock/express": "2.0.2",
    "babel-jest": "28.1.3",
    "esbuild": "0.23.0",
    "eslint": "9.19.0",
    "eslint-plugin-import": "2.31.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "msw": "1.3.2"
  }
}
